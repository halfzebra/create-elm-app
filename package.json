{
  "name": "create-elm-app",
  "version": "0.0.0-development",
  "description": "Elm apps with zero configuration",
  "keywords": [
    "elm",
    "cli",
    "webpack"
  ],
  "author": "Eduard Kyvenko <eduard.kyvenko@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "bin": {
    "elm-app": "./bin/elm-app-cli.js",
    "create-elm-app": "./bin/create-elm-app-cli.js"
  },
  "scripts": {
    "start": "elm-app start",
    "build": "elm-app build",
    "eject": "elm-app eject",
    "create": "elm-app create",
    "commit": "git-cz",
    "precommit": "standard --verbose | snazzy",
    "test": "npm run -s test:cli && npm run -s test:functional",
    "test:functional": "mocha tests/*.spec.js --reporter spec --timeout 15000",
    "test:cli": "mocha tests/cliAccessibility.js --reporter spec --timeout 15000",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/halfzebra/create-elm-app"
  },
  "bugs": {
    "url": "https://github.com/halfzebra/create-elm-app/issues",
    "email": "eduard.kyvenko@gmail.com"
  },
  "files": [
    "bin",
    "config",
    "scripts",
    "template",
    "README.md"
  ],
  "dependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^6.7.7",
    "chalk": "1.1.3",
    "clean-webpack-plugin": "^0.1.16",
    "cli-table": "0.3.1",
    "connect-history-api-fallback": "^1.3.0",
    "cross-spawn": "^5.1.0",
    "css-loader": "^0.28.0",
    "dotenv": "^4.0.0",
    "elm": "^0.18.0",
    "elm-hot-loader": "0.5.4",
    "elm-test": "0.18.2",
    "elm-webpack-loader": "^4.3.0",
    "extend": "3.0.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.2",
    "file-loader": "0.11.1",
    "fs-extra": "^2.1.2",
    "html-webpack-plugin": "2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "installed-by-yarn-globally": "^0.1.2",
    "minimist": "1.2.0",
    "postcss-loader": "1.3.3",
    "prompt": "1.0.0",
    "react-dev-utils": "0.5.2",
    "style-loader": "^0.16.1",
    "url-loader": "0.5.8",
    "webpack": "2.3.3",
    "webpack-dev-server": "2.4.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "dir-compare": "^1.3.0",
    "husky": "^0.13.1",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4",
    "semantic-release": "^6.3.2",
    "snazzy": "^7.0.0",
    "standard": "^10.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
