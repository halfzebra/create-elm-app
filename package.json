{
  "name": "create-elm-app",
  "description": "Elm apps with zero configuration",
  "version": "0.0.0-development",
  "author": "Eduard Kyvenko <eduard.kyvenko@gmail.com>",
  "bin": {
    "elm-app": "./bin/elm-app-cli.js",
    "create-elm-app": "./bin/create-elm-app-cli.js"
  },
  "bugs": {
    "url": "https://github.com/halfzebra/create-elm-app/issues",
    "email": "eduard.kyvenko@gmail.com"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@babel/runtime": "7.11.2",
    "assets-webpack-plugin": "3.9.12",
    "autoprefixer": "8.6.5",
    "babel-loader": "8.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "chalk": "4.1.0",
    "cli-table": "0.3.1",
    "connect-history-api-fallback": "1.6.0",
    "copy-webpack-plugin": "5.1.2",
    "cosmiconfig": "5.2.1",
    "cross-spawn": "7.0.3",
    "css-loader": "4.3.0",
    "dotenv": "8.2.0",
    "elm": "latest-0.19.1",
    "elm-asset-webpack-loader": "1.1.2",
    "elm-hot-webpack-loader": "1.1.7",
    "elm-test": "latest-0.19.1",
    "elm-webpack-loader": "6.0.1",
    "file-loader": "6.1.0",
    "fs-extra": "6.0.1",
    "html-webpack-plugin": "4.5.0",
    "http-proxy-middleware": "0.21.0",
    "mini-css-extract-plugin": "0.11.2",
    "minimist": "1.2.5",
    "object-assign": "4.1.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-loader": "3.0.0",
    "postcss-safe-parser": "4.0.2",
    "promise": "8.1.0",
    "prompt": "1.0.0",
    "react-dev-utils": "6.1.1",
    "react-error-overlay": "4.0.1",
    "sockjs-client": "1.4.0",
    "string-replace-loader": "2.3.0",
    "strip-ansi": "6.0.0",
    "style-loader": "1.2.1",
    "uglifyjs-webpack-plugin": "1.3.0",
    "url-loader": "4.1.0",
    "webpack": "4.44.2",
    "webpack-dev-server": "3.11.0",
    "webpack-manifest-plugin": "2.2.0",
    "whatwg-fetch": "3.4.1",
    "workbox-webpack-plugin": "4.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "babel-eslint": "10.1.0",
    "commitizen": "4.2.1",
    "cross-env": "7.0.2",
    "cz-conventional-changelog": "3.3.0",
    "dir-compare": "2.3.0",
    "eslint": "7.10.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "mocha": "7.2.0",
    "nightmare": "3.0.2",
    "prettier": "2.1.2",
    "rimraf": "3.0.2",
    "semantic-release": "17.1.2",
    "shelljs": "0.8.3",
    "unexpected": "11.15.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "bin",
    "config",
    "scripts",
    "template",
    "README.md",
    "!template/build",
    "!template/config/",
    "!template/elm-stuff/",
    "!template/index.html",
    "!template/package.json",
    "!template/scripts/"
  ],
  "keywords": [
    "cli",
    "elm",
    "webpack"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/halfzebra/create-elm-app"
  },
  "scripts": {
    "cz": "git-cz",
    "eslint": "eslint .",
    "format": "prettier --write '{bin,config,scripts,template/src,tests}/**/*.js'",
    "semantic-release": "semantic-release",
    "test": "npm run test:cli && npm run test:functional",
    "test:cli": "mocha tests/cliAccessibility.js --reporter spec --timeout 15000",
    "test:functional": "cross-env UNEXPECTED_FULL_TRACE=true mocha tests/*.spec.js --reporter spec --timeout 15000",
    "app:build": "cd template && node ../bin/elm-app-cli.js build",
    "app:start": "cd template && node ../bin/elm-app-cli.js start",
    "app:test": "cd template && node ../bin/elm-app-cli.js test"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "package-lock.json": [
      "git rm --cached"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
